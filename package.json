    "@volar/typescript": "^2.4.28",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "tsdown": "^0.20.3",
    "tsdown-preset-sxzz": "^0.3.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unplugin-oxc": "^0.5.7",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
